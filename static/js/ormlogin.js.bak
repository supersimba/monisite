window.onload=function () {
    var btnobj=document.getElementById("btn-login");
    var perr=document.getElementById("p-error");
    btnobj.onclick=function () {
        var u=document.getElementById("txt-user");
        var p=document.getElementById("txt-pwd");
        // console.log(u.value);
        if(u.value=="")
        {
            perr.innerHTML="用户名不能为空!";
            return false;
        }else if(p.value=="")
        {
            perr.innerHTML="密码不能为空!";
            return false;
        }
        else {
            alert("commit");
        }
    }
}